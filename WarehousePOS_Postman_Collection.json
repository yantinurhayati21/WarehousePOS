{
  "info": {
    "name": "WarehousePOS API",
    "description": "API Collection for Warehouse & POS System",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://localhost:7001/api",
      "type": "string"
    },
    {
      "key": "externalAuthUrl",
      "value": "https://api-wms.wit.co.id",
      "type": "string"
    },
    {
      "key": "token",
      "value": "",
      "type": "string"
    },
    {
      "key": "appToken",
      "value": "",
      "type": "string"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{token}}",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "1. Generate Token (External API Direct)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.token) {",
                  "    pm.collectionVariables.set('appToken', jsonData.token);",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"app_name\": \"wms\",\n  \"app_key\": \"wms-app-key\",\n  \"device_id\": \"postman\",\n  \"device_type\": \"00031310\",\n  \"fcm_token\": \"\",\n  \"ip_address\": \"0.0.0.0\"\n}"
            },
            "url": {
              "raw": "{{externalAuthUrl}}/GenerateToken",
              "host": ["{{externalAuthUrl}}"],
              "path": ["GenerateToken"]
            }
          }
        },
        {
          "name": "2. Login (External API Direct)",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "token",
                "value": "{{appToken}}",
                "description": "Token from GenerateToken step"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"identifier\": \"test@wit.id\",\n  \"password\": \"123456\"\n}"
            },
            "url": {
              "raw": "{{externalAuthUrl}}/Login",
              "host": ["{{externalAuthUrl}}"],
              "path": ["Login"]
            }
          }
        },
        {
          "name": "Generate Token (via Local API)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.token) {",
                  "    pm.collectionVariables.set('appToken', jsonData.token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"appName\": \"wms\",\n  \"appKey\": \"wms-app-key\",\n  \"deviceId\": \"postman\",\n  \"deviceType\": \"00031310\",\n  \"fcmToken\": \"\",\n  \"ipAddress\": \"0.0.0.0\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/Auth/generate-token",
              "host": ["{{baseUrl}}"],
              "path": ["Auth", "generate-token"]
            }
          }
        },
        {
          "name": "Login (via Local API)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.appToken) {",
                  "    pm.collectionVariables.set('appToken', jsonData.appToken);",
                  "}",
                  "if (jsonData.userToken) {",
                  "    pm.collectionVariables.set('token', jsonData.userToken);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"identifier\": \"test@wit.id\",\n  \"password\": \"123456\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/Auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["Auth", "login"]
            }
          }
        },
        {
          "name": "Full Auth (Generate + Login)",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.appToken) {",
                  "    pm.collectionVariables.set('appToken', jsonData.appToken);",
                  "}",
                  "if (jsonData.userToken) {",
                  "    pm.collectionVariables.set('token', jsonData.userToken);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"identifier\": \"test@wit.id\",\n  \"password\": \"123456\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/Auth/full-auth",
              "host": ["{{baseUrl}}"],
              "path": ["Auth", "full-auth"]
            }
          }
        }
      ]
    },
